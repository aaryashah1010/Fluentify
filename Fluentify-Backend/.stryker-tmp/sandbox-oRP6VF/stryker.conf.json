{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "testRunner": "command",
  "commandRunner": {
    "command": "npm test"
  },
  "reporters": [
    "progress",
    "clear-text",
    "html"
  ],
  "coverageAnalysis": "off",
  "mutate": [
    "src/services/**/*.js",
    "src/utils/**/*.js",
    "src/middlewares/**/*.js",
    "!src/**/*.test.js",
    "!src/utils/server.js",
    "!src/utils/test-gemini.js"
  ]
}