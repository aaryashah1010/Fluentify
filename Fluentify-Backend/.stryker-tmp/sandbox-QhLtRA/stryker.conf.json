{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "testRunner": "jest",
  "coverageAnalysis": "perTest",
  "concurrency": 4,
  "mutate": [
    "src/services/**/*.js",
    "src/utils/**/*.js",
    "src/middlewares/**/*.js",
    "!src/**/*.test.js",
    "!src/utils/server.js",
    "!src/utils/test-gemini.js"
  ]
}