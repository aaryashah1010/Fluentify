{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "testRunner": "command",
  "mutate": [
    "src/controllers/**/*.js",
    "src/services/**/*.js",
    "src/repositories/**/*.js",
    "src/utils/**/*.js",
    "!src/test/**/*.js"
  ],
  "commandRunner": {
    "command": "npm test"
  },
  "reporters": ["progress", "clear-text", "html"],
  "coverageAnalysis": "off"
}
